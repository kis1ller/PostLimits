{
  "manifest_version": 2,
  "name": "Left Posts",
  "version": "0.0.1",
  "description": "Количество оставшихся постов на сегодня",
  "icons": {
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "background": {
    "page": "background.html",
    "persistent": true
  },
  "permissions": [
    "tabs",
    "webRequest",
    "webRequestBlocking",
    "https://*/*",
    "http://*/*",
    "activeTab",
    "notifications"
  ],
  "browser_action": {
    "default_title": "Left Posts",
    "default_popup": "popup.html"
  }
}